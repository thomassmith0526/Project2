{{#each jobs as |job| }}
<Div style="border-style: solid; margin-bottom: 5px; display:flex;" class="JobField">
  <section class="job-column1">  
    <ul>Job Id: {{job.id}}</ul>
    <ul>Client:{{job.client.last_name}}, {{job.client.first_name}}</ul>
    <ul>Employee:<b style="color: red">MAKE THIS WORK</b></ul> 
  </section>
  <section class="job-column2">  
    <ul>Task:{{job.task_id}}</ul>
    <ul>Date:{{job.date}}</ul>
    <ul>Comments:{{job.comments}}</ul>
  </section>
  <section>
    <ul>isDone:{{job.isDone}}</ul>
    <add-to-calendar-button
      name="Client:{{job.client.last_name}}, {{job.client.first_name}}"
      options="'Google'"
      location="{{job.client.location}}"
      startDate="{{job.date}}"
      endDate="{{job.date}}"
      startTime="08:00"
      endTime="18:00"
      timeZone="America/Los_Angeles"
    ></add-to-calendar-button>
  </section>  
</Div>
{{/each}}
